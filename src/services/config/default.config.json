{
  "server": {
    "host": "0.0.0.0",
    "port": 3000,
    "baseUrl": "http://localhost:3000",
    "publicUrl": "http://localhost:3000",
    "cors": {
      "enabled": true,
      "origin": "*",
      "credentials": false
    },
    "session": {
      "cookieName": "authToken",
      "secure": false,
      "sameSite": "lax"
    },
    "admin": {
      "enabled": true,
      "loginPath": "/admin/login",
      "gateAll": true
    },
    "rateLimit": {
      "enabled": false,
      "windowMs": 60000,
      "max": 100
    }
  }
}
