{
  "server": {
  "port": 4000,
  "baseUrl": "http://localhost:4000",
  "publicUrl": "http://localhost:4000",
    "cors": {
      "enabled": true,
      "origin": [
        "https://cosy.world",
        "https://www.cosy.world",
        "https://cosyworld.app"
      ],
      "credentials": false
    },
    "rateLimit": {
      "enabled": true,
      "windowMs": 60000,
      "max": 120
    },
    "session": {
      "cookieName": "authToken",
      "secure": true,
      "sameSite": "lax"
    }
  }
}
