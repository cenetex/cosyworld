{
  "name": "cosyworld",
  "version": "0.0.12",
  "description": "AI Avatar Discord bot with persistent memory, turnâ€‘based combat, and Crossmint NFT support",
  "type": "module",
  "private": true,
  "main": "src/index.mjs",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "WEB_PORT=3001 cross-env NODE_ENV=development DEBUG=awilix:resolver nodemon src/index.mjs --delay 3000",
    "start": "cross-env NODE_ENV=production node src/index.mjs",
    "serve:prod": "npm run start",
    "reset-setup": "node scripts/reset-setup.mjs",
    "build": "npm run clean && npm run build:js && npm run docs",
    "clean": "node -e \"require('fs').rmSync('dist', {recursive: true, force: true})\"",
    "build:js": "npx webpack --mode production --progress --stats=errors-only",
    "docs": "node scripts/build-docs.mjs",
    "docs:api": "jsdoc -c jsdoc.config.json",
    "docs:api:watch": "nodemon --watch src --exec 'npm run docs:api'",
    "docs:validate": "eslint src/**/*.mjs --no-eslintrc --parser-options=ecmaVersion:2023,sourceType:module --rule 'valid-jsdoc: [error, {requireReturn: false}]'",
    "docs:coverage": "node scripts/check-jsdoc-coverage.mjs",
    "docs:all": "npm run docs && npm run docs:api",
    "task": "node scripts/task.mjs",
    "sync:nft": "node scripts/task.mjs sync:collection",
    "update:models": "node scripts/task.mjs update:models",
    "backfill:agent-ids": "node scripts/task.mjs backfill:agent-ids",
    "migrate:agent-blocks": "node scripts/task.mjs migrate:agent-blocks",
    "migrate:agent-events": "node scripts/task.mjs migrate:agent-events",
  "migrate:collections": "node scripts/migrateCollectionFields.mjs",
    "test:agent-events": "node scripts/testAgentEvents.mjs",
    "lint": "npx eslint .",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "analyze": "npx webpack --mode production --profile --json > webpack-stats.json",
    "smoke:ai": "node scripts/smoke-ai.mjs",
    "test:global-x": "node scripts/test-global-x-posting.mjs",
    "check:version": "node scripts/check-version-bump.mjs",
    "prepare": "node scripts/install-git-hooks.mjs",
    "deploy:setup-db": "node scripts/create-indexes.js",
    "deploy:verify": "node scripts/verify-phase2-deployment.js",
    "deploy:prepare": "npm run lint && npm run build:js && npm run deploy:setup-db && npm run deploy:verify",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "discord",
    "ai",
    "avatar",
    "rpg"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cenetex/cosyworld.git"
  },
  "author": "Jon Beckwith",
  "license": "MIT",
  "dependencies": {
    "@coinbase/cdp-sdk": "^1.38.6",
    "@google/genai": "^1.29.1",
    "@google/generative-ai": "^0.24.1",
    "@modelcontextprotocol/sdk": "^1.22.0",
    "@modelcontextprotocol/server-memory": "^2025.9.25",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "arweave": "^1.15.7",
    "autoprefixer": "^10.4.22",
    "awilix": "^12.0.5",
    "axios": "^1.13.2",
    "bs58": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "copy-webpack-plugin": "^13.0.1",
    "cors": "^2.8.5",
    "csv-stringify": "^6.6.0",
    "discord.js": "^14.24.2",
    "dotenv": "^17.2.3",
    "ethers": "^6.15.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "fuse.js": "^7.1.0",
    "globby": "^16.0.0",
    "helius-sdk": "^2.0.2",
    "js-sha3": "^0.9.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^7.0.0",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "ollama": "^0.6.3",
    "openai": "^6.9.0",
    "postcss": "^8.5.6",
    "replicate": "^1.3.1",
    "sharp": "^0.34.5",
    "string-similarity": "^4.0.4",
    "tailwindcss": "^4.1.17",
    "telegraf": "^4.16.3",
    "tweetnacl": "^1.0.3",
    "twitter-api-v2": "^1.28.0",
    "uuid": "^13.0.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@vitest/coverage-v8": "^4.0.9",
    "@vitest/ui": "^4.0.9",
    "babel-loader": "^10.0.0",
    "c8": "^10.1.3",
    "core-js": "^3.46.0",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "docdash": "^2.0.2",
    "eslint": "^9.39.1",
    "jsdoc": "^4.0.5",
    "markdown-it": "^14.1.0",
    "mini-css-extract-plugin": "^2.9.4",
    "nodemon": "^3.1.11",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "sinon": "^21.0.0",
    "style-loader": "^4.0.0",
    "supertest": "^7.1.4",
    "vitest": "^4.0.9",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  }
}
